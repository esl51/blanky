{
  "name": "blanky",
  "version": "0.0.2",
  "description": "blank frontend gulp bootstrap",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:esl51/blank.git"
  },
  "browserslist": [
    "defaults"
  ],
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development gulp",
    "build": "NODE_ENV=production gulp build",
    "pw": "NODE_ENV=production gulp pw",
    "lint:scripts": "npx eslint --fix --ext .js --ignore-path .gitignore ./src/scripts",
    "lint:styles": "npx stylelint --fix ./src/styles",
    "lint:html": "npx eslint --fix --ext .html,.html.twig --ignore-path .gitignore ./src/html",
    "lint:bem": "npx bemlint src/html/*.html.twig --mod='--' --errors",
    "lint": "npm run lint:scripts & npm run lint:styles & npm run lint:html & npm run lint:bem"
  },
  "author": "Maxim Zhgulev",
  "license": "MIT",
  "dependencies": {
    "animated-scroll-to": "^2.3.0",
    "glightbox": "^3.2.0",
    "include-media": "^2.0.0",
    "normalize.css": "^8.0.1",
    "notyf": "^3.10.0",
    "ymaps": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/register": "^7.21.0",
    "@html-eslint/eslint-plugin": "^0.26.0",
    "@html-eslint/parser": "^0.26.0",
    "autoprefixer": "^10.4.14",
    "babelify": "^10.0.0",
    "bemlint": "^1.7.0",
    "browser-sync": "^3.0.2",
    "browserify": "^17.0.0",
    "core-js": "^3.30.1",
    "del": "^7.0.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sonarjs": "^1.0.4",
    "gulp": "^5.0.0",
    "gulp-cache": "^1.1.3",
    "gulp-clean-css": "^4.3.0",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^9.0.0",
    "gulp-postcss": "^10.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-terser": "^2.1.0",
    "gulp-twig": "^1.2.0",
    "gulp-webp": "^5.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2",
    "penthouse": "^2.3.3",
    "postcss": "^8.4.22",
    "prettier": "^3.2.5",
    "sass": "^1.62.0",
    "sharp": "^0.33.3",
    "sharp-ico": "^0.1.5",
    "stylelint": "^16.3.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-prettier": "^5.0.0",
    "through2": "^4.0.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  }
}
