{
  "env": {
    "browser": true
  },
  "extends": [
    "standard",
    "plugin:sonarjs/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "sonarjs/cognitive-complexity": ["error", 50]
  }
}
